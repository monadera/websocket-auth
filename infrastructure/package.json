{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write .",
    "formatcheck": "prettier --check .",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.130.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "aws-cdk": "2.130.0",
    "eslint": "^8.57.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.7.1"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": false,
    "importOrder": [
      "^(?![.~])",
      "^~/",
      "^./",
      "^../"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
